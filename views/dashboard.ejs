    <div class="card">
      <a href="/user?userid=<%= userdata.user_id %>"> </a>
      <div>Name : <%= userdata.first_name %>
          <%= userdata.last_name %>
      </div>
      <div>Email : <%= userdata.user_email %>
      </div>
